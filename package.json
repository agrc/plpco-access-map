{
  "name": "plpco-video-widget",
  "version": "1.4.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@agrc/sherlock": "^2.2.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "bootstrap": "^4.5.2",
    "esri-loader": "^2.15.0",
    "jquery": "^3.5.1",
    "node-sass": "^4.14.1",
    "query-string": "^6.13.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3"
  },
  "scripts": {
    "start": "REACT_APP_DEPLOY=DEV react-scripts start",
    "build": "react-scripts build",
    "test": "REACT_APP_DEPLOY=DEV react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "release": "standard-version",
    "deploy-stage": "REACT_APP_DEPLOY=VIEWER npm run build && wonkavision clean && wonkavision zip && wonkavision ship ./deploy/deploy.zip wwwroot/plpco-videos/viewer && wonkavision unzip wwwroot/plpco-videos/viewer",
    "build-zip": "npm run build && wonkavision zip ./build ./deploy $REACT_APP_DEPLOY.zip",
    "build-prod": "wonkavision clean && REACT_APP_DEPLOY=VIEWER npm run build-zip && REACT_APP_DEPLOY=INTERNAL npm run build-zip && REACT_APP_DEPLOY=BELLWETHER npm run build-zip"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/node-logger": "^6.0.21",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.21",
    "babel-loader": "^8.1.0",
    "react-is": "^16.13.1",
    "standard-version": "^9.0.0"
  }
}
