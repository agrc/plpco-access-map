{
  "name": "plpco-videos",
  "version": "1.13.10",
  "private": true,
  "type": "module",
  "scripts": {
    "build:prod": "vite build --mode viewer_prod && vite build --mode internal_prod --outDir dist/internal --base /internal/ && vite build --mode bellwether_prod --outDir dist/bellwether --base /bellwether/",
    "build:stage": "vite build --mode viewer_stage && vite build --mode internal_stage --outDir dist/internal --base /internal/ && vite build --mode bellwether_stage --outDir dist/bellwether --base /bellwether/",
    "format": "prettier --write .",
    "lint": "eslint . --report-unused-disable-directives",
    "preview": "vite preview",
    "release": "standard-version",
    "start": "vite",
    "storybook": "storybook dev",
    "test": "vitest"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports",
      "prettier-plugin-packagejson"
    ],
    "printWidth": 120,
    "singleQuote": true
  },
  "dependencies": {
    "@arcgis/core": "^4.34.8",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.2.0",
    "@react-hook/window-size": "^3.1.1",
    "bootstrap": "^5.3.8",
    "clsx": "^2.1.1",
    "downshift": "^9.2.0",
    "lodash.escaperegexp": "^4.1.2",
    "lodash.sortby": "^4.7.0",
    "lodash.uniqwith": "^4.5.0",
    "query-string": "^9.3.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-error-boundary": "^6.1.0",
    "react-fast-compare": "^3.2.2",
    "reactstrap": "^9.2.3"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^10.2.8",
    "@storybook/addon-links": "^10.2.8",
    "@storybook/react-vite": "^10.2.8",
    "@ugrc/eslint-config": "^1.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "eslint": "^9.39.2",
    "eslint-plugin-storybook": "10.2.8",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^3.0.0",
    "prop-types": "^15.8.1",
    "sass": "^1.97.3",
    "storybook": "^10.2.8",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
