{
  "name": "plpco-videos",
  "version": "1.13.5",
  "private": true,
  "type": "module",
  "scripts": {
    "build:prod": "vite build --mode viewer_prod && vite build --mode internal_prod --outDir dist/internal --base /internal/ && vite build --mode bellwether_prod --outDir dist/bellwether --base /bellwether/",
    "build:stage": "vite build --mode viewer_stage && vite build --mode internal_stage --outDir dist/internal --base /internal/ && vite build --mode bellwether_stage --outDir dist/bellwether --base /bellwether/",
    "format": "prettier --write .",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "release": "standard-version",
    "start": "vite",
    "storybook": "storybook dev",
    "test": "vitest"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports",
      "prettier-plugin-packagejson"
    ],
    "printWidth": 120,
    "singleQuote": true
  },
  "dependencies": {
    "@arcgis/core": "^4.32.10",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@react-hook/window-size": "^3.1.1",
    "bootstrap": "^5.3.6",
    "clsx": "^2.1.1",
    "downshift": "^9.0.9",
    "lodash.escaperegexp": "^4.1.2",
    "lodash.sortby": "^4.7.0",
    "lodash.uniqwith": "^4.5.0",
    "query-string": "^9.1.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-fast-compare": "^3.2.2",
    "reactstrap": "^9.2.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@ugrc/eslint-config": "^1.2.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.27.0",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.14",
    "prop-types": "^15.8.1",
    "sass": "^1.89.0",
    "storybook": "^8.6.14",
    "vite": "^6.3.5",
    "vitest": "^3.1.4"
  }
}
