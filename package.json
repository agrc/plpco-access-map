{
  "name": "plpco-videos",
  "version": "1.8.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build --mode viewer && vite build --mode internal --outDir dist/internal --base /internal/ && vite build --mode bellwether --outDir dist/bellwether --base /bellwether/",
    "format": "prettier --write .",
    "lint": "eslint ./src",
    "preview": "vite preview",
    "release": "standard-version",
    "start": "vite",
    "storybook": "start-storybook",
    "test": "vitest"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "extends": [
      "eslint:recommended",
      "prettier",
      "plugin:react/recommended",
      "plugin:react/jsx-runtime",
      "plugin:react-hooks/recommended"
    ],
    "ignorePatterns": [
      "dist/**"
    ]
  },
  "dependencies": {
    "@arcgis/core": "^4.24.7",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@react-hook/window-size": "^3.0.7",
    "bootstrap": "^5.2.0",
    "clsx": "^1.2.1",
    "downshift": "^6.1.9",
    "eslint-config-prettier": "^8.5.0",
    "jquery": "^3.6.1",
    "lodash.escaperegexp": "^4.1.2",
    "lodash.sortby": "^4.7.0",
    "lodash.uniqwith": "^4.5.0",
    "node-sass": "^7.0.1",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fast-compare": "^3.2.0",
    "reactstrap": "^9.1.4"
  },
  "devDependencies": {
    "@mdx-js/react": "^2.1.3",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/react": "^6.5.10",
    "@vitejs/plugin-react": "^2.0.1",
    "eslint-plugin-react": "^7.31.6",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.1",
    "prettier-plugin-packagejson": "^2.2.18",
    "sass": "^1.54.8",
    "standard-version": "^9.5.0",
    "vite": "^3.1.4",
    "vitest": "^0.22.1"
  }
}
