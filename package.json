{
  "name": "plpco-videos",
  "version": "1.13.4",
  "private": true,
  "type": "module",
  "scripts": {
    "build:prod": "vite build --mode viewer_prod && vite build --mode internal_prod --outDir dist/internal --base /internal/ && vite build --mode bellwether_prod --outDir dist/bellwether --base /bellwether/",
    "build:stage": "vite build --mode viewer_stage && vite build --mode internal_stage --outDir dist/internal --base /internal/ && vite build --mode bellwether_stage --outDir dist/bellwether --base /bellwether/",
    "format": "prettier --write .",
    "lint": "eslint ./src",
    "preview": "vite preview",
    "release": "standard-version",
    "start": "vite",
    "storybook": "storybook dev",
    "test": "vitest"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "plugins": [
      "prettier-plugin-organize-imports",
      "prettier-plugin-packagejson"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "extends": [
      "eslint:recommended",
      "prettier",
      "plugin:react/recommended",
      "plugin:react/jsx-runtime",
      "plugin:react-hooks/recommended"
    ],
    "ignorePatterns": [
      "dist/**"
    ]
  },
  "dependencies": {
    "@arcgis/core": "^4.30.9",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@react-hook/window-size": "^3.1.1",
    "bootstrap": "^5.3.3",
    "clsx": "^2.1.1",
    "downshift": "^9.0.8",
    "eslint-config-prettier": "^9.1.0",
    "lodash.escaperegexp": "^4.1.2",
    "lodash.sortby": "^4.7.0",
    "lodash.uniqwith": "^4.5.0",
    "node-sass": "^9.0.0",
    "query-string": "^9.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.1.2",
    "react-fast-compare": "^3.2.2",
    "reactstrap": "^9.2.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^8.4.1",
    "@storybook/addon-essentials": "^8.4.1",
    "@storybook/addon-links": "^8.4.1",
    "@storybook/react": "^8.4.1",
    "@storybook/react-vite": "^8.4.1",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.3",
    "sass": "^1.80.5",
    "storybook": "^8.3.4",
    "vite": "^5.4.10",
    "vitest": "^2.1.4"
  }
}
