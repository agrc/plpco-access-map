{
  "name": "plpco-videos",
  "version": "1.8.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@arcgis/core": "^4.23.7",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@react-hook/window-size": "^3.0.7",
    "bootstrap": "^5.1.3",
    "clsx": "^1.1.1",
    "downshift": "^6.1.7",
    "eslint-config-prettier": "^8.5.0",
    "esri-loader": "^3.5.0",
    "jquery": "^3.6.0",
    "lodash.escaperegexp": "^4.1.2",
    "lodash.sortby": "^4.7.0",
    "lodash.uniqwith": "^4.5.0",
    "node-sass": "^7.0.1",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fast-compare": "^3.2.0",
    "reactstrap": "^9.1.1"
  },
  "scripts": {
    "build": "vite build --mode viewer && vite build --mode internal --outDir dist/internal --base /internal/ && vite build --mode bellwether --outDir dist/bellwether --base /bellwether/",
    "format": "prettier --write .",
    "preview": "vite preview",
    "release": "standard-version",
    "start": "vite",
    "storybook": "start-storybook",
    "test": "vitest"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "extends": [
      "prettier",
      "plugin:react/recommended",
      "plugin:react/jsx-runtime",
      "plugin:react-hooks/recommended"
    ],
    "ignorePatterns": [
      "dist/**"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120
  },
  "devDependencies": {
    "@mdx-js/react": "^2.1.1",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-vite": "^0.1.36",
    "@storybook/react": "^6.5.9",
    "@vitejs/plugin-react": "^1.3.2",
    "prettier-plugin-organize-imports": "^3.0.0",
    "sass": "^1.53.0",
    "standard-version": "^9.5.0",
    "vite": "^2.9.12",
    "vite-plugin-html-env": "^1.1.3",
    "vitest": "^0.15.1"
  }
}
